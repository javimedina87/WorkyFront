{
  "manifest_version": 2,
  "name": "Mi primera extension",
  "description":"Descripcion de la extension",
  "browser_action": {
  "default_title":"Hello pomodoro"
  },
  "externally_connectable": {
    "matches": ["*://localhost/*"]
  },
  "version":"1",
  "background": {
  "scripts":["background.js"]
  },
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}
